<div id="mobileBody" style="display: none;">
  <div class="mobile-body-contents">
    <ul>
      <% postsList.forEach(function(post){ %>
        <hr>
        <h2><%= post.name %></h2>
         <p class="font2"><%= post.content %></p>
         <br>
      <% }); %>
    </ul>
  </div>
</div>

<div id="desktopBody" style="display: none;">
  <div class="desktop-body-contents">
    <ul>
      <% postsList.forEach(function(post){ %>
        <hr>
        <h2><%= post.name %></h2>
         <p class="font2"><%= post.content %></p>
         <br>
      <% }); %>
    </ul>
  </div>
</div>
<script src="/js/home.js"></script>